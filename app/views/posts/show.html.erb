<div>
	<h1><%= @post.title %></h1>
</div>
<div>
	<em>Created <%= @post.created_at %></em>
</div>
<div>
	<p>
		<%= @post.body %>
	</p>
</div>
</br>
<div>
  <%= link_to("Delete This Post", post_path(@post.id), method: :delete) %>
</div>
<br>
<div>
  <%= link_to("Back to List of Posts", posts_path) %>
</div>
